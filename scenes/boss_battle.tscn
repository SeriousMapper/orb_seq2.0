void fragment() {
	float size = 100.0;
	vec2 pos = floor(1.0 / size * FRAGCOORD.xy);
	float color = 0.0;
	float starValue = rand(pos);

	if (rand(SCREEN_UV.xy / 10.0) > 0.996)
	{
		float r = rand(SCREEN_UV.xy) * 2.0;
		color = r * (0.85 * sin(TIME * (r * 0.4) + 100.0 * r) + 0.2);
	}
	COLOR = vec4(vec3(color),1.0) + bg_color;
}"

